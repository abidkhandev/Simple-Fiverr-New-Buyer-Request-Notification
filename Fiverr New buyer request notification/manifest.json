{
    "manifest_version": 2,
    "name": "Fiverr New Buyer Request Notification",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": ["https://www.fiverr.com/users/*/requests"],
            "js": ["jquery.min.js", "content.js"]
        }
    ],
    
    "background": {
        "scripts": ["background.js"]
    },

    "permissions" : ["notifications"]
}